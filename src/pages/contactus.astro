---
import Layout from "../layouts/Layout.astro";
import { CONTACT_US_PAGE } from "../const.js";
import { PHONE_NUMBER_FORMATTED, LOCATION, LOCATION_STATE } from "../const.js";

const { title, description } = CONTACT_US_PAGE;
---

<Layout
	headline="CONTACT US"
	title="Auto Body Shop Rockville - Contact us Today."
	description={`${PHONE_NUMBER_FORMATTED} - Collision Repair & Auto Body in ${LOCATION}, ${LOCATION_STATE} | dents, scratches, bumpers, fenders. We turn Wrecks into Wonders - Call Us Today.`}
/>
